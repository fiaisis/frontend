{
  "name": "fia",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.19 <21 || >=22.12"
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.14.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "7.3.4",
    "@mui/material": "6.1.9",
    "@mui/x-date-pickers": "^8.16.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.9.2",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "axios": "^1.13.1",
    "dayjs": "^1.11.18",
    "express": "5.1.0",
    "express-ws": "5.0.2",
    "i18next": "^25.6.0",
    "jwt-decode": "^4.0.0",
    "loglevel": "1.9.2",
    "monaco-editor": "^0.54.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ga4": "^2.1.0",
    "react-i18next": "^13.3.1",
    "react-json-pretty": "2.2.0",
    "react-monaco-editor": "^0.55.0",
    "react-router-dom": "5.3.0",
    "react-scripts": "5.0.0",
    "single-spa-react": "4.6.1",
    "typescript": "5.9.x",
    "ws": "8.18.3"
  },
  "resolutions": {
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2"
  },
  "lint-staged": {
    "src/**/*.{tsx,js,jsx,json}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "cypress/**/*.{tsx,js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "lint:js": "eslint --ext=tsx --ext=js --ext=jsx --fix ./src",
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "build:standalone": "cross-env BUNDLE_REACT=true vite build",
    "preview": "vite preview --port 5001",
    "test": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "test:watch": "react-scripts test --env=jsdom --watch",
    "serve:build": "yarn build && vite preview --port 5001",
    "serve:backend": "node server/server.js",
    "analyze": "yarn build && source-map-explorer build/main.*",
    "postinstall": "husky install",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "run-frontend": "yarn build && cd ../scigateway && yarn start",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{tsx,js,jsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/setupTests.js",
      "!src/testbed/**/*"
    ]
  },
  "devDependencies": {
    "@cypress/react": "^7.0.3",
    "@cypress/webpack-dev-server": "^5.1.3",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^10.1.0",
    "cypress": "^15.5.0",
    "cypress-real-events": "^1.15.0",
    "eslint": "8.x",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-cypress": "5.2.0",
    "eslint-plugin-prettier": "5.5.4",
    "html-webpack-plugin": "^5.6.4",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "serve": "14.2.5",
    "vite": "^7.1.12"
  }
}
