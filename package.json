{
  "name": "scigateway-demo-plugin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "7.1.0",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "5.15.13",
    "@mui/material": "5.15.13",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.28",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "express": "4.18.3",
    "express-ws": "5.0.2",
    "i18next": "^23.10.1",
    "loglevel": "1.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.1.0",
    "react-json-pretty": "2.2.0",
    "react-router-dom": "6.22.3",
    "react-scripts": "5.0.1",
    "single-spa-react": "6.0.1",
    "typescript": "5.4.2",
    "ws": "8.16.0"
  },
  "resolutions": {
    "@types/react": "18.2.66",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0"
  },
  "lint-staged": {
    "src/**/*.{tsx,js,jsx,json}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "cypress/**/*.{tsx,js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "lint:js": "eslint --ext=tsx --ext=js --ext=jsx --fix ./src",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --env=jsdom --coverage --watchAll=false",
    "test:watch": "craco test --env=jsdom --watch",
    "serve:build": "yarn build && serve -l 5001 build",
    "serve:backend": "node server/server.js",
    "analyze": "yarn build && source-map-explorer build/main.*",
    "postinstall": "husky install",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{tsx,js,jsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/setupTests.js",
      "!src/testbed/**/*"
    ]
  },
  "devDependencies": {
    "@cypress/react": "^8.0.0",
    "@cypress/webpack-dev-server": "^3.5.1",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "cypress": "^13.7.0",
    "cypress-real-events": "^1.11.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-prettier": "5.1.3",
    "html-webpack-plugin": "^5.5.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "serve": "14.2.1"
  }
}
